@model RecommendVm
@{
    ViewData["Title"] = "Recommend Page";
}

<div class="text-center">
    <h1 class="display-4">You Are @Model.User.Name</h1>
</div>

<div class="text-center">
    <h1 class="display-4">This Movie Are Recommended</h1>
    <table class="table">
        <thead>
            <th>Title</th>
            <th>Genres</th>
        </thead>
        <tbody>
            @foreach(var row in Model.Predicts){
                @if(row.Movie != null){
                    <tr>
                        <td>@row.Movie.Title</td>
                        <td>@row.Movie</td>
                    </tr>
                }
            }
        </tbody>
    </table>
</div>

